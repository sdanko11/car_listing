<%= simple_form_for @car do |f| %>
  <%= f.input :name label: 'Name' %>
  <%= f.input :color label: 'Color' %>
  <%= f.input :year label: 'Year' %>
  <%= f.input :mileage label: 'Miles' %>
  <%= f.input :description label: 'Description'%>
  <%= f.submit %>
<% end %>

