<%= flash[:car_added] %>
<%= simple_form_for @car do |f| %>
  <%= f.input :name, label: 'Name' %><br>
  <%= f.input :color, label: 'Color' %><br>
  <%= f.input :year, label: 'Year' %><br>
  <%= f.input :mileage, label: 'Miles' %><br>
  <%= f.error :milegae, id: 'Please add valid miles' %>
  <%= f.input :description, label: 'Description'%><br>
  <%= f.submit %>
<% end %>